package com.library.domain;

public class Book {
	private int id;
	private String name;
	private String catalog;
	private String price;
	private String desc;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCatalog() {
		return catalog;
	}
	public void setCatalog(String catalog) {
		this.catalog = catalog;
	}
	public String getPrice() {
		return price;
	}
	public void setPrice(String price) {
		this.price = price;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	@Override
	public String toString() {
		return "Book [id=" + id + ", name=" + name + ", catalog=" + catalog + ", price=" + price + ", desc=" + desc
				+ "]";
	}
	public Book(int id, String name, String catalog, double price, String desc) {
		super();
		this.id = id;
		this.name = name;
		this.catalog = catalog;
		this.price = price;
		this.desc = desc;
	}
	public Book() {
		super();
	}
}
