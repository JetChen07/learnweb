package com.library.domain;

public class Catalog {
	private long id;
	private String name;
	private String desc;
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	@Override
	public String toString() {
		return "Catalog [id=" + id + ", name=" + name + ", desc=" + desc + "]";
	}
	public Catalog(Long id2, String name, String desc) {
		super();
		this.id = id2;
		this.name = name;
		this.desc = desc;
	}
	public Catalog() {
		super();
	}
}
